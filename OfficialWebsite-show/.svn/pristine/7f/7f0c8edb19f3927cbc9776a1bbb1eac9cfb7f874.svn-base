body {
    background-color: #f8f9f9;
}


.background {
    width: 100%;
    height: 900px;
    overflow: hidden;
}

.imgList {
    width: 98%;
    height: 100%;
    position: relative;
}

.imgList>div {
    width: 12%;
    height: 31%;
    border: 1px solid #ccc;
    position: absolute;
    top: 30%;
    left: 42%;
    background: #fff;
    padding: 10px;
}

.imgBig {
    width: 20%;
    height: 53%;
    top: 28%;
    left: 37%;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    z-index: 1000
}

@media screen and (max-width: 1680px) and (min-width:1480px) {
    .imgList>div {
        width: 13%;
        height: 29%;
        border: 1px solid #ccc;
        position: absolute;
        top: 30%;
        left: 42%;
        background: #fff;
        padding: 10px;
    }
}

@media screen and (max-width: 1479px) and (min-width:1280px) {
    .imgList>div {
        width: 13%;
        height: 26%;
        border: 1px solid #ccc;
        position: absolute;
        top: 30%;
        left: 42%;
        background: #fff;
        padding: 10px;
    }
}

