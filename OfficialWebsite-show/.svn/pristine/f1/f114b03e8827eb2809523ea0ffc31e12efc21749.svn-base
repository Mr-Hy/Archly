}



/* 悬浮导航栏 */
.suspend {
    position: absolute;
    right: -10%;
    top: 10%;
    /* position: fixed; */
    transition: 0.5s;
    z-index: 999;
}

.stickBtn {
    cursor: pointer;
    width: 10px;
}

.stickBtn img {
    cursor: pointer;
}

.suspendItem {
    width: 116px;
    height: 198px;
    background: #2dadfe;
    opacity: 0.7;
    border: 1px solid #d0dae2;
    position: absolute;
    top: 27px;
    left: -29px;
}

.supendList {
    width: 100%;
    height: 100%;
    list-style: none;
    text-align: center;
}

.supendList li {
    margin: 0 auto;
    cursor: pointer;
    width: 90%;
    height: 49px;
    line-height: 2.5vw;
    color: #fff;
    border-bottom: 0.5px dotted #e8e8e8;
    font-size: 12pt;
    letter-spacing: 2px;
}

.Astyle {
    text-decoration: none;
    color: #fff;
}

