/* 后台返回信息弹窗 */
.msgText {
    width: 100%;
    min-width: 1280px;
    height: 100%;
    /* background-color: rgba(0, 0, 0, 0.5); */
    z-index: 99999;
    position: fixed;
    display: none;
}

.msgDiv {
    width: 400px;
    height: 250px;
    background-color: #fff;
    box-shadow: 0px 0px 5px 1px rgba(0, 0, 0, 0.5);
    z-index:9999;
    margin: 10% auto;
    border-radius: 5px;
}

.msgDivTxt {
    position: absolute;
    left: 48.5%;
    top: 32%;
    font-size: 18px;
    font-weight: 700;
}

.msgContent {
    width: 80%;
    position: relative;
    top: 90px;
