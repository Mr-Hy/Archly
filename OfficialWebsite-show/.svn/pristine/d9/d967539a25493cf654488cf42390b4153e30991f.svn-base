    text-align: center;
}

.closeMsgDiv {
    width: 80px;
    height: 30px;
    color: #fff;
    line-height: 30px;
    background-color: #409EFF;
    border-radius: 3px;
    position: relative;
    top: 160px;
    margin: auto;
    text-align: center;
    cursor: pointer;
}